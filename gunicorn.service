[Unit]
Description=gunicorn service
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/testproject-aws
ExecStart=/home/ubuntu/.local/share/virtualenvs/testproject-aws-mf332VLe/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/testproject-aws/testproject.sock core.wsgi:application

[Install]
WantedBy=multi-user.target
